                  <div class="tab-pane fade in show active" id="panel21" role="tabpanel">
                    <h5 class="my-2 h5">Overview</h5>
                    <hr><h2>{{name}}'s Outstanding Bills</h2><hr>

                    <ul>
                      {{#each bills}}
                      {{#if unpaid}}
                      {{> overviewPanel}}
                      {{/if}}
                      {{/each}}
                    </ul>

                  <hr><h2>Paid/Settled</h2><hr>

                    <ul>
                      {{#each bills}}
                      {{#if paid}}
                      {{> overviewPanel}}
                      {{/if}}
                      {{/each}}
                    </ul>

                  </div>
                  <!-- Panel 2 -->
                  <div class="tab-pane fade" id="panel22" role="tabpanel">
                    <h5 class="my-2 h5">Upcoming Bills</h5><hr>
                    <ul>
                      {{#each bills}}
                      {{#if unpaid}}
                      {{> upcomBill}}
                      {{/if}}
                      {{/each}}
                    </ul>
                  </div>
                  <!-- Panel 3 -->
                  <div class="tab-pane fade" id="panel23" role="tabpanel">
                    <h5 class="my-2 h5">Payment History</h5><hr>
                      <ul>
                        {{#each payments}}
                          {{> paymHistory}}
                        {{/each}}
                      </ul>
                  </div>
                </div>
              </div>
            </div>
          </div>

